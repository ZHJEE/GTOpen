FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 60 35
HEADER "创建焊盘"

TILE


GROUP "参数:"
GLOC 1 3
GSIZE 37 25
ENDGROUP


TEXT "焊盘类型"
TLOC 3 1
ENDTEXT
POPUP <padtype_pop> "" ""
FIELD padtype
FLOC 11 1
ENUMSET 8
POP "padtype_pop"
ENDFIELD

TEXT "单位"
TLOC 23 1
ENDTEXT
POPUP <unitsel_pop> "mm" "mm","mil" "mil"
FIELD unitsel
FLOC 27 1
ENUMSET 4
POP "unitsel_pop"
ENDFIELD


FIELD pad_view
THUMBNAIL
FLOC 3 6
FSIZE 15 10
ENDFIELD


FIELD rec_pad
FLOC 20 7
CHECKLIST "矩形焊盘" "smd"
ENDFIELD

FIELD cir_pad
FLOC 20 10
CHECKLIST "圆形焊盘" "smd"
ENDFIELD

FIELD obl_pad
FLOC 20 13
CHECKLIST "椭圆焊盘" "smd"
ENDFIELD

FIELD cir_cir_hole
FLOC 20 6
CHECKLIST "金属圆形(盘圆形)" "hole"
ENDFIELD

FIELD cir_Rec_hole
FLOC 20 8
CHECKLIST "金属圆形(盘方形)" "hole"
ENDFIELD

FIELD cir_oval_hole
FLOC 20 10
CHECKLIST "金属圆形(盘椭圆)" "hole"
ENDFIELD

FIELD oval_oval_hole
FLOC 20 12
CHECKLIST "金属椭圆形(盘椭圆)" "hole"
ENDFIELD

FIELD nth_hole
FLOC 20 14
CHECKLIST "非金属圆形" "hole"
ENDFIELD

TEXT "焊盘名称:"
TLOC 2 18
ENDTEXT
FIELD pad_name
FLOC 10 18
STRFILLIN 20 50
ENDFIELD

TEXT "W"
TLOC 3 21
INFO W_name 7
ENDTEXT
FIELD value_W
FLOC 5 21
REALFILLIN 4 4
ENDFIELD


TEXT "H" 
TLOC 12 21
INFO H_name 7
ENDTEXT
FIELD value_H
FLOC 14 21
REALFILLIN 4 4
ENDFIELD

TEXT "D1" 
TLOC 3 24
INFO D1_name 7
ENDTEXT
FIELD value_D1
FLOC 5 24
REALFILLIN 4 4
ENDFIELD

TEXT "D2" 
TLOC 12 24
INFO D2_name 7
ENDTEXT
FIELD value_D2
FLOC 14 24
REALFILLIN 4 4
ENDFIELD

TEXT "阻焊+:"
TLOC 21 21
ENDTEXT
FIELD sold_val
FLOC 26 21
REALFILLIN 4 4
ENDFIELD


